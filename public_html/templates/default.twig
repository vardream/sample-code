<body>
{% if mode.developing == false %}
  {# Load Facebook SDK for JavaScript  #}
{% endif %}

<div class="body__wrapper clear_fix">
  <div class="top_bar">
    <ul class="top_bar_phones">
      <li><span><i class="fa fa-phone color-brand"></i></span> 044 353-94-00</li>
      <li><span><i class="va va-lifecell-sign color-lifecell"></i></span> 063 508-78-85</li>
      <li><span><i class="va va-mts-sign color-mts"></i></span> 066 715-45-15</li>
      <li><span><i class="va va-kyivstar-sign color-kyivstar"></i></span> 067 102-33-78</li>
      <li><a href="https://t.me/mirra_kos" target="_blank"><i class="tgme_logo"></i></a></li>
    </ul>
    <div class="top_bar_items">
      {# Корзина #}
      <div id="appShoppingCartInfo">
        <shopping-cart-info>
          <a class="shopping_cart_block" href="/order">
            <div class="shopping_cart_block__icon">
              <i class="fa fa-shopping-bag fa-2x"></i>
              <div class="shopping_cart_block__count">
                <span>{{ cart.total }}</span>
              </div>
            </div>
            <div class="shopping_cart_block__content">
              <p class="shopping_cart_block__text">Покупки</p>
              <p class="shopping_cart_block__total"><span>{{ cart.sum }}</span> грн.</p>
            </div>
          </a>
        </shopping-cart-info>
      </div>
    </div>
  </div>
  <div id="content">
    <div id="content_left">
      <div id="header">
        <a href="/"></a>
        <div class="band"><a href="/howto">ПОЛУЧИ МАКСИМАЛЬНУЮ СКИДКУ</a></div>
      </div>
      <div class="separator8px_white">&nbsp;</div>
      {# Главное меню вверху #}
      {% spaceless %}
        <div class="menu_top">
          {% if menu_chapters %}
            <ul>
              {% for item in menu_chapters.items %}
                {% if item.url == current %}
                  <li><a href="{{ item.url }}"><span class='current'>{{ item.title }}</span></a>
                {% else %}
                  <li><a href="{{ item.url }}">{{ item.title }}</a>
                {% endif %}
                </li>
              {% endfor %}
            </ul>
          {% endif %}
        </div>
      {% endspaceless %}
      <div class="separator8px_white">&nbsp;</div>
      <div id="article">
        <div id="article_left">
          {% block sidebar_left_top %}{% endblock %}
          {% block before_sidebar_left %}
            <div class="block_left">
              <p class="invitation"><a href="/howto"><img src="/images/img-181226-01.jpg" srcset="/images/img-181226-01@2x.jpg 2x" alt="Получи максимальную скидку. Клуб MIRRA."></a></p>
            </div>
          {% endblock %}
          {% block sidebar_left %}
            {# Боковое меню "Новости" #}
            {% if menu_news %}
              <div class="block_left">
                <p class="menu_header_left">{{ menu_news.title }}</p>
                {% for item in menu_news.items %}
                  <p class="menu_item_left"><strong class="chars_orange">{{ item.date }}</strong><br/>
                    <a href="{{ item.url }}" target="{{ item.target }}"
                       class="{% if current == item.url %}menu_item_left_current{% else %}menu_item_left{% endif %}">{% autoescape false %}{{ item.title }}{% endautoescape %}</a>
                  </p>
                {% endfor %}
                <p class="menu_item_left text-right">
                  <a href="/news" class="menu_item_left color-action">все новости
                    <i class="fa fa-chevron-circle-right"></i>
                  </a>
                </p>
              </div>
            {% endif %}
            {# Боковое меню "Это интерестно" #}
            {% if menu_interesting %}
              <div class="block_left">
                <p class="menu_header_left">{{ menu_interesting.title }}</p>
                {% for item in menu_interesting.items %}
                  <p class="menu_item_left">
                    <a href="{{ item.url }}" target="{{ item.target }}"
                       class="{% if current == item.url %}menu_item_left_current{% else %}menu_item_left{% endif %}">{% autoescape false %}{{ item.title }}{% endautoescape %}</a>
                  </p>
                {% endfor %}
                <p class="menu_item_left text-right"><a href="/interesting" class="menu_item_left"
                                                        style="color:#FF6600">все статьи
                    <i class="fa fa-chevron-circle-right"></i></a></p>
              </div>
            {% endif %}
            {# Боковое меню "Блог" #}
            {% if menu_blog %}
              {% spaceless %}
                <div class="block_left">
                  <p class="menu_header_left">{{ menu_blog.title }}</p>
                  {% for item in menu_blog.items %}
                    <p class="menu_item_left">
                      <a href="{{ item.url }}"
                         class="{% if current == item.url %}menu_item_left_current{% else %}menu_item_left{% endif %}">{% autoescape false %}
                          {% if item.image %}<img src="{{ item.image }}"
                                                  {% if item.image2x %}srcset="{{ item.image2x }} 2x"{% endif %}
                                                  alt="{{ item.title }}">{% endif %}
                          {{ item.title }}{% endautoescape %}</a>
                    </p>
                  {% endfor %}
                  <p class="menu_item_left text-right">
                    <a href="/blog" class="menu_item_left color-action">все статьи
                      <i class="fa fa-chevron-circle-right"></i>
                    </a>
                  </p>
                </div>
              {% endspaceless %}
            {% endif %}
            {# Меню "Косметические линии #}
            {% if menu_lines %}
              <div class="block_left">
                <p class="menu_header_left">{{ menu_lines.title }}</p>
                {% for item in menu_lines.items %}
                  <a href="{{ item.url }}"
                     class="{% if current == item.url %}menu_item_left_current{% else %}menu_item_left{% endif %}">{{ item.title }}</a>
                {% endfor %}
              </div>
            {% endif %}
            {# Меню: Назначение (tags_purpose) #}
            {% if tags_purpose %}
              <div class="block_left">
                <p class="menu_header_left">{{ tags_purpose.title }}</p>
                {% for item in tags_purpose.items %}
                  <a href="{{ item.url }}"
                     class="{% if current == item.url %}menu_item_left_current{% else %}menu_item_left{% endif %}">{{ item.title }}</a>
                {% endfor %}
              </div>
            {% endif %}
          {% endblock %}
        </div>
        <div id="article_right">
          {# Контент #}
          {% block content %}
            <p class="p_h1">{{ content.title }}</p>
            {% if chain %}
              <div class="chain">
                {% for item in chain.items %}
                  {% if loop.last %}<a href="{{ item.url }}" class="chain_link">{{ item.title }}</a>
                  {% else %}<a href="{{ item.url }}"
                               class="chain_link">{{ item.title }}</a> • {% endif %}
                {% endfor %}
              </div>
            {% endif %}
            {% autoescape false %}{{ content.body }}{% endautoescape %}
          {% endblock %}
        </div>
      </div>
    </div>
    {# Правая панель #}
    <div id="content_right">
      {# Форма подписки #}
      <div class="subscribe__container">
        <p class="subscribe__button">Рассылка <i class="fa fa-caret-down"></i></p>
        <form class="subscribe__form">
          <p>Подпишитесь на нашу рассылку акций, новинок и специальных предложений.</p>
          <input id="subscriber_mail" name="subscriber_mail" type="email" maxlength="64"
                 placeholder="введите свой e-mail" required value="">
          <div class="text-center">
            <button type="submit" value="subscribe">Подписаться</button>
          </div>
        </form>
      </div>
      {# Информация по заказам #}
